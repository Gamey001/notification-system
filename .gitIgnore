# =========================
# NestJS / Node.js .gitignore
# =========================

# Environment files
.env
.env.*.local

# Dependency directories
node_modules/

# Build output
dist/
build/
coverage/

# Logs
logs/
*.log
npm-debug.log*
yarn-debug.log*
yarn-error.log*

# OS-specific files
.DS_Store
Thumbs.db

# IDE / Editor folders
.vscode/
.idea/
*.swp
*.swo

# TypeScript cache and output
*.tsbuildinfo
*.d.ts

# Testing
coverage/
.jest/
nyc_output/

# Temporary files
tmp/
temp/
*.tmp
*.bak

# Debugger files
*.pid
*.seed

# Lockfiles (optional if you want to ignore specific ones)
# yarn.lock
# package-lock.json

# Docker
docker-compose.override.yml

# Prisma (if using)
prisma/dev.db

# Optional: Ignore local scripts or secrets
scripts/local/
secrets/
